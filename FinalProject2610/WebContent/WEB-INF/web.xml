<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>FinalProject</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>PassengerServlet</servlet-name>
    <servlet-class>airTraffic.PassengerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PassengerServlet</servlet-name>
    <url-pattern>/passenger</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>JourneyServlet</servlet-name>
    <servlet-class>airTraffic.JourneyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>JourneyServlet</servlet-name>
    <url-pattern>/journey</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FlightSegmentServlet</servlet-name>
    <servlet-class>airTraffic.FlightSegmentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FlightSegmentServlet</servlet-name>
    <url-pattern>/flightSegment</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Flights</servlet-name>
    <servlet-class>airTraffic.FlightSegmentServletAirline</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Flights</servlet-name>
    <url-pattern>/flights</url-pattern>
  </servlet-mapping>
</web-app>